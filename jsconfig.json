{
  "typeAcquisition": {
      "include": [
          "jest"
      ]
  },
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@app/*": ["src"],
      "@handler/*": ["src/handler/*"],
      "@api/*": ["src/api/*"],
      "@config/*": ["src/config/*"],
      "@middlewares/*": ["src/middlewares/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}